{
  "name": "@app/preload",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/_virtual_browser.mjs"
    },
    "./exposed.mjs": {
      "types": "./src/exposed.d.ts",
      "default": "./dist/exposed.mjs"
    }
  },
  "files": ["dist/**", "package.json"],
  "scripts": {
    "build": "vite build",
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@app/automation": "workspace:*",
    "@app/core": "workspace:*",
    "@internal/electron-versions": "*",
    "@internal/eslint-config": "workspace:*",
    "@internal/prettier-config": "workspace:*",
    "@internal/tsconfig": "workspace:*",
    "@internal/vitest-config": "workspace:*",
    "mlly": "1.8.0",
    "typescript": "catalog:dev",
    "vite": "7.1.4"
  },
  "prettier": "@internal/prettier-config"
}
